{
    "name": "express.ts",
    "version": "1.0.0",
    "description": "node with express boilerplate",
    "main": "./bin/startup.js",
    "author": "tB",
    "license": "beerware",
    "engines": {
        "node": "8.4.0",
        "npm": "5.3.0"
    },
    "scripts": {
        "x_preinstall": "sudo npm i -g typescript jest tslint npm-run-all",
        "start": "node ./bin/bootstrap.js",
        "run": "npm run clean && npm run build && clear && npm run start",
        "run:win": "npm run build && npm run start",
        "watch": "tsc --watch",
        "monitor": "nodemon ./bin/bootstrap.js",
        "serve": "npm run clean && run-p watch monitor",
        "serve:win": "run-p watch monitor",
        "lint": "tslint -c tslint.json './src/**/*.{ts,tsx}' --format msbuild",
        "test": "jest --runInBand",
        "build": "tsc -p .",
        "clean": "rm ./bin -rf",
        "autofix": "tslint --fix --project .",
        "publish": "git push origin master"
    },
    "devDependencies": {
        "@types/axios": "^0.14.0",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.2",
        "@types/jasmine": "^3.5.0",
        "@types/jest": "^24.0.24",
        "@types/lodash": "^4.14.149",
        "@types/node": "^12.12.21",
        "@types/uuid": "^3.4.6",
        "jest": "^24.9.0",
        "nodemon": "^2.0.2",
        "npm-check-updates": "^4.0.1",
        "npm-run-all": "^4.1.5",
        "ts-jest": "^24.2.0",
        "typescript": "^3.7.4"
    },
    "dependencies": {
        "async-delay": "^1.0.3",
        "asyncawait": "^3.0.0",
        "axios": "^0.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "git-rev": "^0.2.1",
        "inversify": "^5.0.1",
        "jasmine-core": "^3.5.0",
        "minimist": "^1.2.0",
        "moq.ts": "^3.0.0",
        "os": "^0.1.1",
        "reflect-metadata": "^0.1.13",
        "socket.io": "^2.3.0",
        "uuid": "^3.3.3"
    }
}
